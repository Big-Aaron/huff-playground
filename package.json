{
  "name": "huff-playground",
  "version": "1.0.0",
  "description": "A playground for HyVM",
  "scripts": {
    "start": "webpack-dev-server --hot --config webpack.config.js --mode development",
    "build": "rm -rf dist && webpack --config webpack.config.js --mode production",
    "release": "yarn build && cd dist && git init && git add -A && git commit -m\"initial\" && git remote add origin git@github.com:oguimbal/huff.git && git push --set-upstream origin master --force",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "author": "Olivier Guimbal",
  "repository": "https://github.com/oguimbal/hyvm-playground",
  "license": "MIT",
